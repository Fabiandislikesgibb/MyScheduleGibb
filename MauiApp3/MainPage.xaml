<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:mauiApp3="using:MauiApp3"
            x:Class="MauiApp3.MainPage">

    <TabbedPage.Children>
        <NavigationPage Title="Info" IconImageSource="info.png">
            <x:Arguments>
                <mauiApp3:InfoPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Help" IconImageSource="help.png">
            <x:Arguments>
                <mauiApp3:HelpPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Calendar" IconImageSource="calendar.png">
            <x:Arguments>
                <mauiApp3:CalendarPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Input" IconImageSource="input.png">
            <x:Arguments>
                <mauiApp3:InputPage />
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
</TabbedPage>